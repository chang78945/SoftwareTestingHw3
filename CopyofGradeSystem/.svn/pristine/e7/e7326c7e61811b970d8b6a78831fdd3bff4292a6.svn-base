
public class Grades 
{
	private String ID;
	private String name;
	private int lab1Score;
	private int lab2Score;
	private int lab3Score;
	private int midTermScore;
	private int finalExamScore;
	
	private int totalGrade;
	public void calculateTotalGrade(double lab1,double lab2,double lab3,double midTerm,double finalExam)
	{
		totalGrade=(int)Math.round(lab1Score*lab1+lab2Score*lab2+lab3Score*lab3+midTermScore*midTerm+finalExamScore*finalExam);
	}
	public Grades ()//建構子 
	{ 
		
	} 
	public void setID(String iD) {
		ID = iD;
	}
	public String getID() {
		return ID;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getName() {
		return name;
	}
	public int getTotalGrade() {
		return totalGrade;
	}
	public void setTotalGrade(int totalGrade) {
		this.totalGrade = totalGrade;
	}
	public void setFinalExamScore(int finalExamScore) {
		this.finalExamScore = finalExamScore;
	}
	public void setLab1Score(int lab1Score) {
		this.lab1Score = lab1Score;
	}
	public void setLab3Score(int lab3Score) {
		this.lab3Score = lab3Score;
	}
	public void setLab2Score(int lab2Score) {
		this.lab2Score = lab2Score;
	}
	public void setMidTermScore(int midTermScore) {
		this.midTermScore = midTermScore;
	}
	public int getFinalExamScore() {
		return finalExamScore;
	}
	public int getLab1Score() {
		return lab1Score;
	}
	public int getLab2Score() {
		return lab2Score;
	}
	public int getLab3Score() {
		return lab3Score;
	}
	public int getMidTermScore() {
		return midTermScore;
	}

}
//class Grades 儲存 ID, name, lab1, lab2, lab3, midTerm, finalExam, and totalGrade
